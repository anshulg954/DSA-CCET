#include<iostream>
using namespace std;

int main()
{
	int n,m,c,l;
	cout<<"Enter Number of elements:";
	cin>>n;
	cout<<endl;
	int a[n];
	cout<<"Enter elements:"<<endl;
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	cout<<"Item to be searched:";
	cin>>m;
	for(int i=0;i<n;i++)
	{
		if(a[i]==m)
		{
			c=i;
			l=1;
		}
	}
	cout<<"Index at which item is present=";
	if(l!=1)
	{
        cout<<"-1";
	}
	else
	{
        cout<<c;
	}
	return 0;
}

